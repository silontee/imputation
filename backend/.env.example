APP_ENV=prod
API_PREFIX=/api/v1

# Domain (SSL 인증서 발급에 사용)
DOMAIN=your-domain.com

# Database
DATABASE_URL=postgresql+psycopg2://imputex:imputex_password_change_me@postgres:5432/imputex

# Redis
REDIS_URL=redis://redis:6379/0

# File storage (로컬 경로, NAS 마운트 경로로 변경 가능)
UPLOAD_DIR=/app/uploads
RESULT_DIR=/app/results

# CORS (프론트엔드 도메인)
CORS_ALLOW_ORIGINS=https://your-domain.com

# Limits
MAX_UPLOAD_MB=500
SAMPLE_ROWS=1000
